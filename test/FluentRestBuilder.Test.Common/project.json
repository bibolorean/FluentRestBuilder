{
  "buildOptions": {
    "additionalArguments": [ "/additionalfile:../../stylecop.json" ],
    "keyFile": "../../FluentRestBuilder.snk",
    "nowarn": [
      "CS1591"
    ],
    "warningsAsErrors": true,
    "xmlDoc": true
  },
  "dependencies": {
    "FluentRestBuilder": {
      "target": "project"
    },
    "Microsoft.AspNetCore.Mvc.Abstractions": "1.1.0",
    "Microsoft.EntityFrameworkCore.InMemory": "1.1.0",
    "Microsoft.Extensions.DependencyInjection": "1.1.0",
    "Microsoft.CodeAnalysis.FxCopAnalyzers": {
      "version": "1.2.0-beta2",
      "type": "build"
    },
    "StyleCop.Analyzers": {
      "version": "1.0.0",
      "type": "build"
    },
    "NETStandard.Library": "1.6.1"
  },
  "frameworks": {
    "net451": {},
    "netstandard1.6": {}
  }
}
