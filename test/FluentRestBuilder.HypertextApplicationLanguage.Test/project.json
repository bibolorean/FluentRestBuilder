{
  "buildOptions": {
    "additionalArguments": [ "/additionalfile:../../stylecop.json" ],
    "keyFile": "../../FluentRestBuilder.snk",
    "nowarn": [
      "CS1591"
    ],
    "warningsAsErrors": true,
    "xmlDoc": true
  },
  "dependencies": {
    "xunit": "2.2.0-beta4-build3444",
    "dotnet-test-xunit": "2.2.0-preview2-build1029",
    "FluentRestBuilder.HypertextApplicationLanguage": {
      "target": "project"
    },
    "FluentRestBuilder.Test.Common": {
      "target": "project"
    },
    "Microsoft.DotNet.InternalAbstractions": "1.0.0",
    "Microsoft.CodeAnalysis.FxCopAnalyzers": {
      "version": "1.2.0-beta2",
      "type": "build"
    },
    "StyleCop.Analyzers": {
      "version": "1.0.0",
      "type": "build"
    },
    "NETStandard.Library": "1.6.1"
  },
  "frameworks": {
    "netcoreapp1.0": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.1.0",
          "type": "platform"
        },
        "Microsoft.CodeCoverage": "1.0.2"
      }
    },
    "net451": {}
  },
  "testRunner": "xunit"
}
